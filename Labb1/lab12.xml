<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.cecilialagerwall.labb1.MainActivity">
    <!--name-->
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textPersonName"
        android:text="@string/name"
        android:ems="5"
        android:id="@+id/editText5"
        tools:ignore="LabelFor,TextViewEdits"
        android:enabled="false"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true" />

    <SeekBar
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/seekBar"
        android:layout_marginLeft="70dp"
        tools:ignore="RtlHardcoded"
        android:layout_alignBottom="@+id/editText3"
        android:layout_alignParentEnd="true"
        android:layout_toEndOf="@+id/editText3" />
    
    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textPersonName"
        android:ems="10"
        android:id="@+id/editText2"
        tools:ignore="LabelFor,RelativeOverlap"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true" />

    <!--password-->
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textPersonName"
        android:text="@string/losenord"
        android:ems="5"
        android:id="@+id/editText"
        tools:ignore="LabelFor,TextViewEdits"
        android:enabled="false"
        android:layout_marginTop="24dp"
        android:layout_below="@+id/editText2"
        android:layout_alignParentStart="true" />

    <!--age-->
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textPersonName"
        android:text="@string/alder"
        android:ems="5"
        android:id="@+id/editText3"
        tools:ignore="LabelFor,TextViewEdits"
        android:enabled="false"
        android:layout_marginTop="87dp"
        android:layout_below="@+id/editText4"
        android:layout_alignParentStart="true" />

    <!--epost-->
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textPersonName"
        android:text="@string/epost"
        android:ems="10"
        android:id="@+id/editText7"
        tools:ignore="LabelFor,TextViewEdits"
        android:enabled="false"
        android:layout_below="@+id/editText4"
        android:layout_alignParentStart="true"
        android:layout_marginTop="19dp" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textPassword"
        android:ems="10"
        android:id="@+id/editText4"
        tools:ignore="LabelFor"
        android:layout_below="@+id/editText2"
        android:layout_alignParentEnd="true" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textEmailAddress"
        android:ems="10"
        android:id="@+id/editText6"
        tools:ignore="LabelFor"
        android:layout_alignTop="@+id/editText7"
        android:layout_alignParentEnd="true" />

</RelativeLayout>
